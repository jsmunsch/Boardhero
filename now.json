{
  "name": "boardhero",
  "version": 2,
  "builds": [
    { "src": "client/build/**/*", "use": "@now/static" },
    { "src": "index.js", "use": "@now/node-server" }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "github": {
    "autoAlias": false
  }
}
