{
  "name": "boardhero",
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "description": "",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "dev:server": "npm run build && cd .. && npm start",
    "server": "nodemon index.js",
    "start": "node index.js",
    "heroku-prebuild": "cd client && npm ci --only=prod",
    "reset": "killall -9 node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsmunsch/boardhero"
  },
  "author": "Jonas Munsch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsmunsch/boardhero"
  },
  "homepage": "https://github.com/jsmunsch/boardhero#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.3.2",
    "node": "^12.13.0",
    "now": "^16.4.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2"
  }
}
